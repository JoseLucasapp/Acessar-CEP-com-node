<meta charset="utf-8">
<html>
    <head>
        <title>2112chat</title>
        <link rel="stylesheet" href="./style/style.css" />
    </head>
    <body>
        <div class="root">
            <!--Chat-->
            <div class="chat" id="divChat">
                <div class="campo_msg" id="campo_msg">
                    <ul class="msg" id="msg"></ul>
                </div>
                <form id="formmsg">
                    <input maxlength="50" type="text" id="message" />
                    <button disabled id='sendbtn'></button>
                </form>
            </div>
            <div id="online" class="online">
                <div class="ondiv"></div><span id="users"></span>
            </div>
        </div>
            
        <script src="/socket.io/socket.io.js"></script>
        <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
        <script src="scripts/chat_script.js"></script>
    </body>
</html>